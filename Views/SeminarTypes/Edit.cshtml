@model SeminarManagementSystem.Models.SeminarType

@{
    ViewBag.Title = "Edit";
}

<div>
    <h1 class="text-center text-info fw-bold">Edit Seminar Type</h1>
    <hr />
</div>
<div class="div-center">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div>
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            @Html.HiddenFor(model => model.Id)

            <div>
                @Html.LabelFor(model => model.TypeName)
                <div class="col-md-10">
                    @Html.EditorFor(model => model.TypeName)
                    @Html.ValidationMessageFor(model => model.TypeName, "", new { @class = "text-danger" })
                </div>
            </div>
            <br />
            <div class="div-center">
                <input type="submit" value="Save" class="button button-edit" />
                @Html.ActionLink("Back to List", "Index", new { area = "" }, new { @class = "button button-back" })
            </div>
        </div>
    }

</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
